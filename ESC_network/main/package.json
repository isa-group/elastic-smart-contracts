{
    "name": "ESC_network",
    "version": "1.0.0",
    "description": "ESC_network application implemented in JavaScript",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "child_process": "^1.0.2",
        "csvtojson": "^2.0.10",
        "fabric-ca-client": "^2.1.1",
        "fabric-network": "^2.1.1",
        "postman-request": "^2.88.1-postman.23",
        "protobufjs": "^6.9.0",
        "protobufs": "^0.1.0",
        "random-normal": "^1.0.0",
        "yargs": "^15.3.1"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "eslint": "^5.9.0",
        "mocha": "^5.2.0",
        "nyc": "^14.1.1",
        "sinon": "^7.1.1",
        "sinon-chai": "^3.5.0"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
